@article{cms_collaboration_measurement_2023,
	title = {Measurement of inclusive and differential cross sections for single top quark production in association with a W boson in proton-proton collisions at \${\textbackslash}sqrt\{s\}\$ = 13 {TeV}},
	volume = {2023},
	issn = {1029-8479},
