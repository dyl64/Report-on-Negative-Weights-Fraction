@article{atlas_collaboration_measurement_2017,
	title = {Measurement of jet activity produced in top-quark events with an electron, a muon and two \$b\$-tagged jets in the final state in \$pp\$ collisions at \${\textbackslash}sqrt\{s\}=13\$ {TeV} with the {ATLAS} detector},
	volume = {77},
	issn = {1434-6044, 1434-6052},
	url = {http://arxiv.org/abs/1610.09978},
	doi = {10.1140/epjc/s10052-017-4766-0},
	abstract = {Measurements of jet activity in top-quark pair events produced in proton--proton collisions are presented, using 3.2 fb\${\textasciicircum}\{-1\}\$ of \$pp\$ collision data at a centre-of-mass energy of 13 {TeV} collected by the {ATLAS} experiment at the Large Hadron Collider. Events are chosen by requiring an opposite-charge \$e{\textbackslash}mu\$ pair and two \$b\$-tagged jets in the final state. The normalised differential cross-sections of top-quark pair production are presented as functions of additional-jet multiplicity and transverse momentum, \$p\_\{{\textbackslash}mathrm T\}\$. The fraction of signal events that do not contain additional jet activity in a given rapidity region, the gap fraction, is measured as a function of the \$p\_\{{\textbackslash}mathrm T\}\$ threshold for additional jets, and is also presented for different invariant mass regions of the \$e{\textbackslash}mu b{\textbackslash}bar\{b\}\$ system. All measurements are corrected for detector effects and presented as particle-level distributions compared to predictions with different theoretical approaches for {QCD} radiation. While the kinematics of the jets from top-quark decays are described well, the generators show differing levels of agreement with the measurements of observables that depend on the production of additional jets.},
	pages = {220},
	number = {4},
	journaltitle = {The European Physical Journal C},
	shortjournal = {Eur. Phys. J. C},
	author = {{ATLAS} Collaboration},
	urldate = {2024-02-06},
	date = {2017-04},
