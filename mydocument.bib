@article{frederix_automation_2009,
	title = {Automation of next-to-leading order computations in {QCD}: the {FKS} subtraction},
	volume = {2009},
	issn = {1029-8479},
	url = {http://stacks.iop.org/1126-6708/2009/i=10/a=003?key=crossref.81033cc98129c1aa13e9af382d69179a},
	doi = {10.1088/1126-6708/2009/10/003},
	shorttitle = {Automation of next-to-leading order computations in {QCD}},
	abstract = {We present the complete automation of the universal subtraction formalism proposed by Frixione, Kunszt, and Signer for the computation of any cross section at the next-to-leading order in {QCD}. Given a process, the only ingredient to be provided externally is the infrared- and ultraviolet-ﬁnite contribution of virtual origin. Our implementation, currently restricted to the case of e+e− collisions, is built upon and works in the same way as {MadGraph}. It is particularly suited to parallel computation, and it can deal with any physical process resulting from a theory implemented in {MadGraph}, thus including the Standard Model as well as Beyond the Standard Model theories. We give results for some sample processes that document the performances of the implementation, and show in particular how the number of subtraction terms has an extremely mild growth with ﬁnal-state multiplicity.},
	pages = {003--003},
	number = {10},
	journaltitle = {Journal of High Energy Physics},
	shortjournal = {J. High Energy Phys.},
	author = {Frederix, Rikkert and Frixione, Stefano and Maltoni, Fabio and Stelzer, Tim},
	urldate = {2024-02-27},
	date = {2009-10-01},
	langid = {english},
	file = {Frederix et al. - 2009 - Automation of next-to-leading order computations i.pdf:/Users/sysugeniusl/Zotero/storage/YCNFKNR2/Frederix et al. - 2009 - Automation of next-to-leading order computations i.pdf:application/pdf},
}

