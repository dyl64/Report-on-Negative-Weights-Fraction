@misc{atlas_collaboration_measurement_2020,
	title = {Measurement of the \$t{\textbackslash}bar\{t\}\$ production cross-section and lepton differential distributions in \$e{\textbackslash}mu\$ dilepton events from \$pp\$ collisions at \${\textbackslash}sqrt\{s\}=13\$ {TeV} with the {ATLAS} detector},
	url = {http://arxiv.org/abs/1910.08819},
	doi = {10.1140/epjc/s10052-020-7907-9},
	abstract = {The inclusive top quark pair (\$t{\textbackslash}bar\{t\}\$) production cross-section \${\textbackslash}sigma\_\{t{\textbackslash}bar\{t\}\}\$ has been measured in proton\$-\$proton collisions at \${\textbackslash}sqrt\{s\}=13\$ {TeV}, using \$36.1\$ fb\${\textasciicircum}\{-1\}\$ of data collected in 2015\$-\$16 by the {ATLAS} experiment at the {LHC}. Using events with an opposite-charge \$e{\textbackslash}mu\$ pair and \$b\$-tagged jets, the cross-section is measured to be: {\textbackslash}begin\{equation\}{\textbackslash}nonumber {\textbackslash}sigma\_\{t{\textbackslash}bar\{t\}\} = 826.4 {\textbackslash}pm 3.6{\textbackslash},{\textbackslash}mathrm\{(stat)\}{\textbackslash} {\textbackslash}pm 11.5{\textbackslash},{\textbackslash}mathrm\{(syst)\}{\textbackslash} {\textbackslash}pm 15.7{\textbackslash},{\textbackslash}mathrm\{(lumi)\}{\textbackslash} {\textbackslash}pm 1.9{\textbackslash},{\textbackslash}mathrm\{(beam)\}{\textbackslash},{\textbackslash}mathrm\{pb\}, {\textbackslash}end\{equation\} where the uncertainties reflect the limited size of the data sample, experimental and theoretical systematic effects, the integrated luminosity, and the {LHC} beam energy, giving a total uncertainty of 2.4\%. The result is consistent with theoretical {QCD} calculations at next-to-next-to-leading order. It is used to determine the top quark pole mass via the dependence of the predicted cross-section on \$m\_t{\textasciicircum}\{{\textbackslash}mathrm\{pole\}\}\$, giving \$m\_t{\textasciicircum}\{{\textbackslash}mathrm\{pole\}\}=173.1{\textasciicircum}\{+2.0\}\_\{-2.1\}\$ {GeV}. It is also combined with measurements at \${\textbackslash}sqrt\{s\}=7\$ {TeV} and \${\textbackslash}sqrt\{s\}=8\$ {TeV} to derive ratios and double ratios of \$t{\textbackslash}bar\{t\}\$ and \$Z\$ cross-sections at different energies. The same event sample is used to measure absolute and normalised differential cross-sections as functions of single-lepton and dilepton kinematic variables, and the results compared with predictions from various Monte Carlo event generators.},
	author = {{ATLAS} Collaboration},
	urldate = {2024-01-29},
	date = {2020-06-16},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1910.08819 [hep-ex]},
	keywords = {High Energy Physics - Experiment, {ATLAS}\_2019\_I1759875},
	file = {ATLAS Collaboration - 2020 - Measurement of the \$tbar t \$ production cross-sec.pdf:/Users/sysugeniusl/Zotero/storage/QXTYPF8U/ATLAS Collaboration - 2020 - Measurement of the \$tbar t \$ production cross-sec.pdf:application/pdf},
}


@article{buckley_general-purpose_2011,
	title = {General-purpose event generators for {LHC} physics},
	volume = {504},
	issn = {03701573},
	url = {http://arxiv.org/abs/1101.2599},
	doi = {10.1016/j.physrep.2011.03.005},
	abstract = {We review the physics basis, main features and use of general-purpose Monte Carlo event generators for the simulation of proton-proton collisions at the Large Hadron Collider. Topics included are: the generation of hardscattering matrix elements for processes of interest, at both leading and nextto-leading {QCD} perturbative order; their matching to approximate treatments of higher orders based on the showering approximation; the parton and dipole shower formulations; parton distribution functions for event generators; non-perturbative aspects such as soft {QCD} collisions, the underlying event and diﬀractive processes; the string and cluster models for hadron formation; the treatment of hadron and tau decays; the inclusion of {QED} radiation and beyond-Standard-Model processes. We describe the principal features of the Ariadne, Herwig++, Pythia 8 and Sherpa generators, together with the Rivet and Professor validation and tuning tools, and discuss the physics philosophy behind the proper use of these generators and tools. This review is aimed at phenomenologists wishing to understand better how parton-level predictions are translated into hadron-level events as well as experimentalists wanting a deeper insight into the tools available for signal and background simulation at the {LHC}.},
	pages = {145--233},
	number = {5},
	journaltitle = {Physics Reports},
	shortjournal = {Physics Reports},
	author = {Buckley, Andy and Butterworth, Jonathan and Gieseke, Stefan and Grellscheid, David and Hoche, Stefan and Hoeth, Hendrik and Krauss, Frank and Lonnblad, Leif and Nurse, Emily and Richardson, Peter and Schumann, Steffen and Seymour, Michael H. and Sjostrand, Torbjorn and Skands, Peter and Webber, Bryan},
	urldate = {2024-01-29},
	date = {2011-07},
	langid = {english},
	eprinttype = {arxiv},
	eprint = {1101.2599 [hep-ex, physics:hep-ph]},
	keywords = {High Energy Physics - Experiment, High Energy Physics - Phenomenology},
	file = {Buckley et al. - 2011 - General-purpose event generators for LHC physics.pdf:/Users/sysugeniusl/Zotero/storage/MM68N5J2/Buckley et al. - 2011 - General-purpose event generators for LHC physics.pdf:application/pdf},
}

